prefix owl: <http://www.w3.org/2002/07/owl#>


# Selects all classes (cls1) where the class is owl class that have a URI containing the ftp URL provided by Webulous using regex. The “i” flag means case insensitive.



SELECT ?cls1 WHERE
{
  ?cls1 a owl:Class .
  FILTER regex(str(?cls1), '^ftp://ftp.ebi.ac.uk', "i") .  
}
