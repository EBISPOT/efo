    <!-- http://purl.obolibrary.org/obo/CL_0000038 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CL_0000038">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CL_0000764"/>
